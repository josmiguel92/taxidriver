<script>
 $('document').ready(function(){
    $('#iconModal .icon-container .icon-name').click(function(e){
        e.preventDefault();
        console.log(this.innerHTML)
        $("#appbundle_infographitem_icon")[0].value = this.innerHTML;
        $('#iconModal').modal('hide')


    });

    $('.infograph-edit').click(function(e){
     e.preventDefault();
     $(".fa-spinner.formLoader")[0].classList.remove('hidden');
     var edit_path = "{{ path('dash_infographitem_edit', {'id':'itemidplaceholder'}) }}"

     console.log(edit_path)
     item_id = this.dataset['itemid']
     current_edit_path = edit_path.replace('itemidplaceholder',item_id)
     console.log(current_edit_path)
     $('#editForm').load(current_edit_path, function () {
         $(".fa-spinner.formLoader")[0].classList.add('hidden');
     })

 });
});


</script>
