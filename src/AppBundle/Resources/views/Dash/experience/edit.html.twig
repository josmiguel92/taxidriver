{% extends 'AppBundle:Dash:base.html.twig' %}

{% set pagename = 'experience' %}

{% block content %}
    <h1>Editar Experiencia</h1>



    <div class="row">
        {{ form_start(edit_form) }}
        {{ form_widget(edit_form) }}
        <input type="submit" value="Guardar" class="form-inline btn btn-primary" id="savebtn" />
        {{ form_end(edit_form) }}
        {{ form_start(delete_form) }}
        <input type="submit" value="Eliminar" class="form-inline btn btn-danger" id="deletebtn">
        {{ form_end(delete_form) }}
    </div>

{% endblock %}
