<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="{{ asset("/images/apple-icon.png") }}">
    <link rel="icon" type="image/png" sizes="96x96" href="{{ asset("/images/favicon_admin.png") }}">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />

    <title>{% if pagetitle is defined %}{ pagetitle }}{% endif %} TaxiDriver's Admin</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />


    {% block stylesheets %}
        <!-- Bootstrap core CSS     -->
        <link href="{{ asset("/css/bootstrap.min.css") }}" rel="stylesheet" />

        <!--  Fonts and icons     -->
        <link href="{{ asset("/css/muli-font.css") }}" rel='stylesheet' type='text/css'>
        <link href="{{ asset("/css/themify-icons.css") }}" rel="stylesheet">
        <link href="{{ asset("/css/font-awesome.css") }}" rel="stylesheet">
    {% endblock stylesheets %}
</head>
<body>
<img style="float: right" src="data:image/png;base64,">

<div class="wrapper">
    <h2 style="width: 80%;">Detalles de Reservaci&oacute;n <small style="font-size: 120%;"> {{booking.id}}</small></h2>

    <p style="">{{ booking.bookingTime | date('d/m/Y h:i') }}</p>

    <hr>
    <table style="width: 100%">
        <tr>
            <td style="width: 45%;">
                <h3>
                    Datos generales
                </h3>
                <div class="card">
                    <p>
                        <b>Tipo de servicio:</b>
                        {{ booking.serviceType }}
                    </p>
                    <p>
                        <b>Fecha/Hora Recogida:</b>
                        {{ booking.pickuptime | date('d/M/Y h:i')}}
                    </p>
                    <p>
                        <b>Lugar de recogida:</b>
                        {% if booking.airport == 'airport' %}
                            {% if booking.airportName %}
                                {{ booking.airportName }}
                            {% else %}
                                Aeropuerto
                            {% endif %}
                            <br>
                            <b>Vuelo:</b> {{ booking.flynumber }}
                        {% elseif booking.airport == 'cruise' %}
                            Crucero
                        {% endif %}
                    </p>

                    <p>
                        <b>Otros detalles:</b>
                        {{ booking.details }}
                    </p>
                    <hr>
                    <p>
                        <b>Destino:</b>{{ booking.targetPlace }}
                    </p>
                    <p>
                        {% if booking.returnpickup %}
                            <b>Viajar&aacute; de regreso:</b>
                            {{ booking.returnpickuptime| date('d/M/Y h:i')}}
                            <br>
                            <b>Lugar de recogida:</b>
                            {{ booking.returnpickupplacce }}
                        {% endif %}

                    </p>
                    {% if booking.timelinetravel %}
                        <p>

                            <b>Varios lugares de destino:</b>
                            {{ booking.timelinetravel }}
                        </p>
                    {% endif %}
                </div>
            </td>
            <td style="width: 5%">&nbsp;</td>
            <td style="width: 45%;">
                <h3>
                    Enlace a la reservaci&oacute;n
                </h3>
                <div class="card">
                    <img src="data:image/png;base64,{{ dataimage}}">
                    <p>
                    Para ver los detalles de esta reservaci&oacute;n siga este enlace:
                        <br>
                        <b>{{ absolute_url(path('purchase_details', {'_token':booking.token})) }}</b>
                    </p>
                </div>
            </td>
        </tr>
        <tr>
            <td style="width: 45%;">
                <h3>
                    Gesti√≥n Interna
                </h3>
                <div class="card">
                    <p>
                        <b>Origen de esta reserva:</b>
                        {{ booking.bookingSource }}
                    </p>
                    <p>
                        <b>Precio: </b>
                        {% if booking.price %}
                        {{ booking.price }} USD
                        {% else %}
                        A&uacute;n no hay un precio
                    </p>
                    {%endif %}
                    <hr>
                    {%if booking.paymentDetails %}
                        <p>
                            <b>Detalles de pago:</b>{{ booking.paymentDetails }}
                        </p>
                    {%endif%}

                    {%if booking.drivermsg %}
                        <p>
                            <b>Mensaje enviado al cliente:</b>{{ booking.drivermsg }}
                        </p>
                    {%endif%}


                    {%if booking.drivername %}
                        <p>
                            <b>Contacto del Chofer:</b>{{ booking.drivername }}
                        </p>
                    {%endif%}


                    {%if booking.accepted %}
                        <p>
                            <b>Reservaci&oacute;n aceptada</b>
                        </p>
                    {%endif%}
                </div>
            </td>

            <td style="width: 5%">&nbsp;</td>

            <td style="width: 45%;">
                <h3>
                    Datos del cliente
                </h3>
                <div class="card">
                    <p>
                        <b>Nombre:</b>
                        {{ booking.fullname }}
                    </p>
                    <p><b>Email:</b>
                        {{ booking.email }}
                    </p>
                    <p><b>Telefono:</b>
                        {{ booking.telephone }}
                    </p>
                    <p>
                        <b>Idioma:</b>
                        {{ booking.bookingLanguage }}

                    </p>
                    <p><b>Cantidad de personas:</b>
                        {{ booking.numpeople }}.
                        {% if booking.children %}
                            Incluyen ni&ntilde;os
                        {% endif %}

                    </p>
                    <p>
                        <b>Comentarios del cliente:</b>

                        {{ booking.comment }}
                    </p>
                </div>
            </td>


        </tr>
        <tr>

            <td style="width: 45%;">
                {% if booking.experience %}
                    <h3>
                        Sobre la Experiencia
                    </h3>
                    <div class="card">
                        <p>
                            <b>{{ booking.experience }}</b>

                            {% if booking.experienceTime %}
                                <br>
                                Durante {{ booking.experienceTime }} horas.
                            {% endif %}
                        </p>

                        <p>
                            {%if booking.experienceTaxi %}
                                <b>Necesita transporte</b>
                            {% endif %}
                        </p>

                    </div>
                {% endif %}
            </td>
        </tr>
    </table>

</div>
</body>
</html>